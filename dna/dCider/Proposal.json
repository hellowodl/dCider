{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "description",
    "options"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "lets get drunk"
      ],
      "pattern": "^(.*)$"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "great proposal"
      ],
      "pattern": "^(.*)$"
    },
    "options": {
      "$id": "#/properties/options",
      "type": "array",
      "title": "The Options Schema",
      "items": {
        "$id": "#/properties/options/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "hella nah",
          "hella yeah"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}